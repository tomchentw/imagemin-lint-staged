{
  "name": "imagemin-lint-staged",
  "version": "0.4.0",
  "description": "imagemin CLI designed for lint-staged usage with sensible defaults",
  "license": "MIT",
  "author": {
    "name": "tomchentw",
    "email": "developer@tomchentw.com",
    "url": "https://github.com/tomchentw"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomchentw/imagemin-lint-staged"
  },
  "bugs": {
    "url": "https://github.com/tomchentw/imagemin-lint-staged/issues"
  },
  "homepage": "https://github.com/tomchentw/imagemin-lint-staged/",
  "bin": "bin/index.js",
  "files": [
    "bin/",
    "lib/",
    "CHANGELOG.md"
  ],
  "keywords": [
    "imagemin",
    "CLI",
    "git hook",
    "compress images",
    "lint-staged"
  ],
  "scripts": {
    "test": "jest --runInBand",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{png,jpeg,jpg,gif,svg}": [
      "node ./bin/index.js",
      "git add"
    ],
    "*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "\\d+\\.\\d+\\.\\d+"
    ]
  },
  "dependencies": {
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "util.promisify": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-jest": "^28.1.1",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
